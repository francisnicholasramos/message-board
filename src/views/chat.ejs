<% displayMessage.forEach((msg) => { %>
    <div class="chat-container">
        <span>
            <span class="author"><%= msg.author_name %></span>
            <span class="created_at"><%= msg.created_at %></span>
        </span>
        <p class="message">
            <%= msg.message %>
        </p>

    </div>
<% }) %>
